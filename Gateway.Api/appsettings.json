{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "testRoute": {
        "ClusterId": "lightningApiCluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "lightningApiCluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7178/"
          }
        }
      }
    }
  }
}